                                    <linearGradient id="gaugeGradient" x1="0%" y1="0%" x2="100%" y2="0%">                <path d="M22 11.08V12C21.9988 14.1564 21.3005 16.2547 20.0093 17.9818C18.7182 19.7088 16.9033 20.9725 14.8354 21.5839C12.7674 22.1953 10.5573 22.1219 8.53447 21.3746C6.51168 20.6273 4.78465 19.2461 3.61096 17.4371C2.43727 15.628 1.87979 13.4881 2.02168 11.3363C2.16356 9.18455 2.99721 7.13631 4.39828 5.49706C5.79935 3.85781 7.69279 2.71537 9.79619 2.24013C11.8996 1.7649 14.1003 1.98232 16.07 2.85999" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>

                                        <stop offset="0%" style="stop-color:#ef4444"/>                <path d="M22 4L12 14.01L9 11.01" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>

                                        <stop offset="50%" style="stop-color:#f59e0b"/>            </svg>

                                        <stop offset="100%" style="stop-color:#10b981"/>            <span id="toastMessage">Message</span>

                                    </linearGradient>        </div>

                                </defs>    </div>

                            </svg>

                            <div class="gauge-center">    <script src="/static/script.js"></script>

                                <div class="gauge-score" id="overallScore">0.0</div></body>

                                <div class="gauge-label" id="overallLabel">Neutral</div></html>

