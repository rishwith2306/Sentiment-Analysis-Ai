                            <div class="report-score" id="lexiconScore">--</div>
                        </div>
                        <div class="report-body">
                            <div class="report-metric">
                                <span class="metric-label">Confidence</span>
                                <div class="metric-bar">
                                    <div class="metric-fill" id="lexiconConfidence"></div>
                                </div>
                                <span class="metric-value" id="lexiconConfidenceValue">--</span>
                            </div>
                            <div class="findings">
                                <h4>Key Findings</h4>
                                <ul id="lexiconFindings"></ul>
                            </div>
                        </div>
                    </div>

                    <!-- VisionAgent Report -->
                    <div class="card card-glass agent-report">
                        <div class="report-header">
                            <div class="report-icon vision">👁️</div>
                            <div>
                                <h3>VisionAgent Report</h3>
                                <p>Visual Sentiment Analysis</p>
                            </div>
                            <div class="report-score" id="visionScore">--</div>
                        </div>
                        <div class="report-body">
                            <div class="report-metric">
                                <span class="metric-label">Confidence</span>
                                <div class="metric-bar">
                                    <div class="metric-fill" id="visionConfidence"></div>
                                </div>
                                <span class="metric-value" id="visionConfidenceValue">--</span>
                            </div>
