                </div>

                <!-- Agent Reports -->
                <div class="agent-reports">
                    <!-- LexiconAgent Report -->
                    <div class="card card-glass agent-report">
                        <div class="report-header">
                            <div class="report-icon lexicon">📝</div>
                            <div>
                                <h3>LexiconAgent Report</h3>
