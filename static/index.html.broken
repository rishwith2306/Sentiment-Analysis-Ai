                            </div>
                        </div>
                    </div>

                    <!-- FusionAgent Report -->
                    <div class="card card-glass agent-report fusion-report">
                        <div class="report-header">
                            <div class="report-icon fusion">🧠</div>
                            <div>
                                <h3>FusionAgent Report</h3>
                                <p>Multimodal Context Analysis</p>
                            </div>
                            <div class="report-score" id="fusionScore">--</div>
                        </div>
                        <div class="report-body">
                            <div class="report-metric">
                                <span class="metric-label">Confidence</span>
                                <div class="metric-bar">
                                    <div class="metric-fill" id="fusionConfidence"></div>
                                </div>
                                <span class="metric-value" id="fusionConfidenceValue">--</span>
                            </div>
                            <div class="findings">
                                <h4>Key Findings</h4>
                                <ul id="fusionFindings"></ul>
                            </div>
                            <div class="fusion-insights" id="fusionInsights"></div>
                        </div>
                    </div>
                </div>

                <!-- Raw JSON Viewer (Collapsible) -->
                <div class="card card-glass json-viewer">
                    <button class="json-toggle" id="jsonToggle">
                        <svg viewBox="0 0 24 24" fill="none">
                            <path d="M19 9l-7 7-7-7" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                        View Complete JSON Receipt
                    </button>
                    <div class="json-content hidden" id="jsonContent">
                        <pre id="jsonOutput"></pre>
                    </div>
                </div>
            </section>

        </div>
    </main>

    <!-- Toast Notification -->
    <div class="toast hidden" id="toast">
        <svg class="toast-icon" viewBox="0 0 24 24" fill="none">
            <path d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
